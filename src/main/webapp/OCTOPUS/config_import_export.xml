				<param name="textQualifier" value="&quot;"/>
				<param name="exportMapping" type="map">
					<param name="*" value="{:*}"/>
					<param name="Dokumenttyp * (Hauptperson)" value="{DTM:*}"/>
					<param name="Dokumenttyp * (Partner)" value="{DTP:*}"/>
				</param>
			</param>
		</param>

