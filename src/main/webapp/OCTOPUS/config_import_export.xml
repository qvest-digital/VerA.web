				<param name="textQualifier" value="&quot;"/>
				<param name="exportMapping" type="map">
					<param name="*" value="{:*}"/>
				</param>
			</param>
		</param>

		<param name="formatCSVPersdat+Anschrift+Doktyp" type="map">
