					-->
				</param>
				<!-- Nutzung der Adressfreitextfelder -->
				<param name="textfieldMapping" type="map">
					<!-- Muster:
					<param name="n:Doctype" value="NAME_DES_DOKUMENTTYPS"/>
					<param name="n:Person" value="IMPORTFELD_PERSON"/>
