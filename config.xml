    <hudson.tasks.Shell>
      <command># this needs /root/.pgpass (chmod 0600) set up
# 127.0.0.1:5432:veraweb:veraweb:password

set -e
ssh root@veraweb-mit.lan.tarent.de &apos;service tomcat7 stop&apos;
ssh root@veraweb-ohne.lan.tarent.de &apos;service tomcat7 stop&apos;
sleep 10
ssh root@veraweb-mit.lan.tarent.de &apos;psql -U veraweb -h 127.0.0.1 veraweb&apos; &lt;files/upgrade.sql
ssh root@veraweb-ohne.lan.tarent.de &apos;psql -U veraweb -h 127.0.0.1 veraweb&apos; &lt;files/upgrade.sql
scp files/l10n/*.resource root@veraweb-mit.lan.tarent.de:/etc/veraweb/l10n/
scp files/l10n/*.resource root@veraweb-ohne.lan.tarent.de:/etc/veraweb/l10n/
ssh root@veraweb-mit.lan.tarent.de &apos;rm -rf /var/lib/tomcat7/webapps/veraweb*&apos;
ssh root@veraweb-ohne.lan.tarent.de &apos;rm -rf /var/lib/tomcat7/webapps/veraweb*&apos;
scp target/veraweb.war root@veraweb-mit.lan.tarent.de:/var/lib/tomcat7/webapps/veraweb.war
scp target/veraweb.war root@veraweb-ohne.lan.tarent.de:/var/lib/tomcat7/webapps/veraweb.war
ssh root@veraweb-mit.lan.tarent.de &apos;service tomcat7 start&apos;
ssh root@veraweb-ohne.lan.tarent.de &apos;service tomcat7 start&apos;
sleep 10
</command>
    </hudson.tasks.Shell>
