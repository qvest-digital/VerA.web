<script type="text/javascript"><!--//--><![CDATA[//><!--
$(function () {
    $('li#gaesteliste').click(function () {
        window.location.href = '${paths.staticWeb}do/ShowGuestList?selectNone=true';
    });

    $('li#gastdetails').click(function () {
        window.location.href = '${paths.staticWeb}do/ShowGuestDetail';
    });

    $('#save_value').click(function () {
        var sel = $('input[type=checkbox]:checked').map(function (_, el) {
            return $(el).val();
        }).get();

        // Enable to see marked fields by sending
        // alert(sel);

        jQuery.ajax({
            url: $("#csv-export").data("csvexport"),
            data: {
                selectedFields: sel
            },
            type: 'GET',
            success: function (data) {
                alert('Tmp: 200');

            },
            error: function (data) {
                alert('Tmp: 500');
            }
        });
    });

    $('.check:button').click(function(){
        var checked = !$(this).data('checked');
        $('input:checkbox').prop('checked', checked);
        $(this).data('checked', checked);
    });
});
//--><!]]></script>

