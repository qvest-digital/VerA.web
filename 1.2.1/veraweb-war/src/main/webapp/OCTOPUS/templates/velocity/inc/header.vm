<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3c.org/TR/1999/REC-html401-19991224/loose.dtd">
<html>
	<head>
		<title>VerA.web - $!title</title>
##		Browser-Cache deaktivieren
		<meta http-equiv="expires" content="0">
		<meta http-equiv="cache-control" content="no-cache">
		<meta http-equiv="pragma" content="no-cache">
##		Utils
		<script type="text/javascript" src="${paths.staticWeb}scripts/utils.js"></script>
		<script type="text/javascript" src="${paths.staticWeb}scripts/clipboard.js"></script>
##		Tabs und Popups
		<script type="text/javascript" src="${paths.staticWeb}scripts/dhtml-tabs.js"></script>
		<script type="text/javascript" src="${paths.staticWeb}scripts/dhtml-popup.js"></script>
		<script type="text/javascript" src="${paths.staticWeb}scripts/dhtml-dialogs.js"></script>
##		Calendar (http://www.dynarch.com/projects/calendar/)
		<script type="text/javascript" src="${paths.staticWeb}jscalendar/calendar.js"></script>
		<script type="text/javascript" src="${paths.staticWeb}jscalendar/calendar-setup.js"></script>
		<script type="text/javascript" src="${paths.staticWeb}jscalendar/lang/calendar-de.js"></script>
		<link rel="stylesheet" type="text/css" href="${paths.staticWeb}jscalendar/calendar-win2k-cold-1.css" media="all">
##		WCH - Windowed Controls Hider (http://www.aplus.co.yu/wch/intro/)
##		<!--[if IE]>
		<script type="text/javascript" src="${paths.staticWeb}scripts/WCH.js"></script>
##		<![endif]-->
##		Style
		<link rel="stylesheet" type="text/css" href="${paths.staticWeb}styles/default.css" media="all">
		<link rel="stylesheet" type="text/css" href="${paths.staticWeb}styles/screen.css" media="screen,projection" title="Ausw&auml;rtiges Amt Intranet-Stil">
##		Style for fix Position (bottom right)
		<style type="text/css">
			/* IE 5.0/Win and other lesser browsers will use this */
			#fixme {
				position: absolute; right: 12px; bottom: 0px;
				left: 173px;
			}
			/* used by Netscape6+/Mozilla, Opera 5+, Konqueror, Safari, OmniWeb 4.5+, ICEbrowser */
			div > div#fixme, td > div#fixme {
				position: fixed;
			}
		</style>
		<!--[if gte IE 5.5]>
		<![if lt IE 7]>
		<style type="text/css">
			/* IE5.5+/Win - this is more specific than the IE 5.0 version */
			div#fixme {
				top: expression((fixLayer() + 0 - fixme.offsetHeight + (document.documentElement.clientHeight ? document.documentElement.clientHeight : document.body.clientHeight) + (ignoreMe = document.documentElement.scrollTop ? document.documentElement.scrollTop : document.body.scrollTop)) + 'px');
				left: 173px;
				width: expression((fixLayer() + -185 + (document.documentElement.clientWidth ? document.documentElement.clientWidth : document.body.clientWidth)) + 'px');
				right: auto;
				bottom: auto;
			}
		</style>
		<![endif]>
		<![endif]-->
	</head>
	<body>
		<table class="header"><tr>
    		<td class="logo1"><img src="${paths.staticWeb}images/aamt_logo.gif" width="150" height="70" alt="AA Logo"/></td>
    		<td class="logo2" style="background-image: url(${paths.staticWeb}images/topbg.gif);">&nbsp;</td>
		</tr><tr>
    		<td class="headerkennung"><img src="${paths.staticWeb}images/intranet_kennung.gif" width="150" height="30" alt="Intranet"/></td>
    		<td class="headertitle"><strong class="headertitle">$!title</strong></td>
		</tr></table>
		
		<table class="content"><tr>
			<td class="menu">
				<div class="menu">
					#parse("inc/menu.vm")					
				</div>
			</td>
			<td class="content">

#if($octopusRequest.task != "ShowPersonCategorieList")
#if($countInsert && $countInsert > 0)
	<div style="margin: 10px 0px 0px 0px; padding: 2px 3px 2px 3px; background-color: #ffc080; border: 1px solid #ff8000;">
		#if($countInsert == 0)
			Es wurde kein neuer Datensatz angelegt.
		#elseif($countInsert == 1)
			Es wurde ein neuer Datensatz angelegt.
		#else
			Es wurden $countInsert neue Datens&auml;tze angelegt.
		#end
	</div>
#end
#if($countUpdate && $countUpdate > 0)
	<div style="margin: 10px 0px 0px 0px; padding: 2px 3px 2px 3px; background-color: #ffc080; border: 1px solid #ff8000;">
		#if($countUpdate == 0)
			Es wurde kein Datensatz ge&auml;ndert.
		#elseif($countUpdate == 1)
			Es wurde ein Datensatz ge&auml;ndert.
		#else
			Es wurden $countUpdate Datens&auml;tze ge&auml;ndert.
		#end
	</div>
#end
#if($countRemove)
	<div style="margin: 10px 0px 0px 0px; padding: 2px 3px 2px 3px; background-color: #ffc080; border: 1px solid #ff8000;">
		#if($countRemove == 0)
			Es wurde kein Datensatz gel&ouml;scht.
		#elseif($countRemove == 1)
			Es wurde ein Datensatz gel&ouml;scht.
		#else
			Es wurden $countRemove Datens&auml;tze gel&ouml;scht.
		#end
	</div>
#end
#end

#if(false)
	</td></tr></table></body></html>
#end
