#set($activeMenu = "Personen")
#set($title = "Status des Importvorgangs")
#parse("inc/header.vm")

<div id="content_container">
    <div id="breadcrumb" class="textRight marginBottom10">
    	<strong>Sie sind hier</strong> <span><a href="${paths.staticWeb}do/" title="Zur Startseite">Home</a> > Personen > <a href="${paths.staticWeb}do/ImportPersons" title="Zum Personen-Import">Personen-Import</a> > Importergebnis</span>
    </div>
    <h1>Importergebnis</h1>
	
	#if($importStatus.dupCount && $importStatus.dupCount != 0)
		<div id="buttonPanel-top" class="textRight marginBottom10">
            <form action="ImportPersonsEditDuplicates" method="post">
                <input type="hidden" name="importId" value="$!importStatus.id">
                #set($saveCount = $!importStatus.saveCount - $!importStatus.dsCount + $!importStatus.dupCount)
                <input type="hidden" name="saveCount" value="$saveCount"/>
                <input type="hidden" name="start" value="0"/>
    			<input type="hidden" name="selectNone" #if($selectNone && $selectNone == "false") value="false" #else value="true" #end/>
                <input type="submit" class="mainButton" value="Dubletten"/>
            </form>
		</div>
	#end
	
	<div id="main_content" class="contentBoxColored grayBorder marginBottom10">
		
		<div class="table width70">
			<div class="tableRow">
				<div class="tableCell inlineLabel width30">
					<strong>Import-ID:</strong>
				</div>
				<div class="tableCell">
					$!message.text($importStatus.id)
				</div>
			</div>
			<div class="tableRow">
				<div class="tableCell">
					<strong>Datens&auml;tze Insgesamt:</strong>
				</div>
				<div class="tableCell">
					$!importStatus.dsCount
				</div>
			</div>
			<div class="tableRow">
				<div class="tableCell">
					<strong>Anzahl Dubletten:</strong>
				</div>
				<div class="tableCell">
					$!importStatus.dupCount
				</div>
			</div>
			<div class="tableRow">
				<div class="tableCell">
					<strong>Datens&auml;tze speicherbar:</strong>
				</div>
				<div class="tableCell">
					$!importStatus.saveCount
				</div>
			</div>
			#if($importStatus.igCount  && $importStatus.igCount != 0)
				<div class="tableRow">
    				<div class="tableCell">
    					<strong>Ignorierte Fehlerdatens&auml;tze:</strong>
    				</div>
    				<div class="tableCell">
    					$!importStatus.igCount
    				</div>
    			</div>
			#end
		</div>
		
	</div>
	
	<form action="ImportPersonsFinalise" method="post">
		<span class="textRight floatRight">
			#if($importStatus.saveCount  && $importStatus.saveCount != 0)
                <input type="hidden" name="importId" value="$!importStatus.id"/>
                <input type="submit" class="mainButton" value="Import finalisieren" onclick="htmlWait();"/>
			#end
			<input type="button" class="mainButton" value="Zur&uuml;ck zu Personenimport" onclick="window.location.href='ImportPersons';"/>
		</span>
	</form>

	<div class="clear"></div>
</div>


#if($imported)
    <script type="text/javascript">
     #if($imported == 0)
    	alert('Sie haben keinen Datensatz importiert.');
     #elseif($imported == 1)
    	alert('Sie haben einen Datensatz importiert.');
     #elseif($imported > 1)
    	alert('Sie haben $imported Datens√§tze importiert.');
     #end
    </script>
#end

#parse("inc/footer.vm")
