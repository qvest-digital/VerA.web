}
textarea.textdisabled {
	width: 98%;
	font-family: sans-serif;
	font-size: 11px;
	color: #606060;
	background-color: #f0f0f0;
	border: 1px solid #336699;
	padding: 1px 2px 1px 2px;
}
input.int {
	width: 60px;
	font-family: sans-serif;
	font-size: 11px;
	color: #000000;
	background-color: #ffffff;
	border: 1px solid #336699;
	padding: 1px 2px 1px 2px;
}
input.intdisabled {
	width: 60px;
	font-family: sans-serif;
	font-size: 11px;
	color: #606060;
	background-color: #f0f0f0;
	border: 1px solid #336699;
	padding: 1px 2px 1px 2px;
}
input.date {
	width: 80px;
	font-family: sans-serif;
	font-size: 11px;
	color: #000000;
	background-color: #ffffff;
	border: 1px solid #336699;
	padding: 1px 2px 1px 2px;
}
input.datedisabled {
	width: 80px;
	font-family: sans-serif;
	font-size: 11px;
	color: #606060;
	background-color: #f0f0f0;
	border: 1px solid #336699;
	padding: 1px 2px 1px 2px;
}
select.date {
	font-family: sans-serif;
	font-size: 11px;
	text-align: right;
	color: #000000;
	background-color: #ffffff;
	border: 1px solid #336699;
}
select.datedisabled {
	font-family: sans-serif;
	font-size: 11px;
	text-align: right;
	color: #606060;
	background-color: #f0f0f0;
	border: 1px solid #336699;
}
input.checkbox {
}
input.checkboxdisabled {
}
input.radiobox {
}
input.radioboxdisabled {
}
select.combobox {
	font-family: sans-serif;
	font-size: 11px;
	width: 99%;
