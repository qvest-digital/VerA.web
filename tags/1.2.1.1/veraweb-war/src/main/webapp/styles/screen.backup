	color: #808080;
	background-color: #ffffff;
	border-top: 2px solid #88aadd;
	border-left: 2px solid #88aadd;
	border-right: 2px solid #336699;
	border-bottom: 2px solid #336699;
}

input.id {
	width: 120px;
	font-family: sans-serif;
	font-size: 11px;
	color: #606060;
	background-color: #f0f0f0;
	border: 1px solid #336699;
	padding: 1px 2px 1px 2px;
}
input.text {
	width: 98%;
	font-family: sans-serif;
	font-size: 11px;
	color: #000000;
	background-color: #ffffff;
	border: 1px solid #336699;
	padding: 1px 2px 1px 2px;
}
input.textdisabled {
	width: 98%;
	font-family: sans-serif;
	font-size: 11px;
	color: #606060;
	background-color: #f0f0f0;
	border: 1px solid #336699;
	padding: 1px 2px 1px 2px;
}
textarea.text {
	width: 98%;
	font-family: sans-serif;
	font-size: 11px;
	color: #000000;
	background-color: #ffffff;
	border: 1px solid #336699;
